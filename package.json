        "eslint-config-airbnb-typescript": "^9.0.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-eslint-comments": "^3.2.0",
        "eslint-plugin-import": "2.20.1",
        "eslint-plugin-prettier": "^3.1.3",
        "eslint-plugin-promise": "^4.2.1",
        "jest": "^26.4.0",
        "nodemon": "^2.0.4",
